se if(minutes>0){return`${minutes}m ${seconds%60}s`}else{return`${seconds}s`}}getAnimation(id){return this.animations.get(id)}getActiveAnimations(){return Array.from(this.animations.values())}isActive(){return this.isRunning}getTargetFPS(){return this.targetFPS}setTargetFPS(fps){this.targetFPS=fps;this.frameInterval=1e3/fps;if(this.isRunning){this.stop();this.start()}}clearAll(){for(const animation of this.animations.values()){if(animation.onComplete){animation.onComplete(animation)}}this.animations.clear();this.stop();this.emit(\"scheduler:cleared\")}dispose(){this.disposed=true;this.clearAll();this.removeAllListeners()}isDisposed(){return this.disposed}}export{AnimationScheduler};\n","warnings":[],"map":{"version":3,"mappings":"kHAKA,OAAS,iBAAoB,SAC7B,OAAS,gBAAmB,aAuDrB,MAAM,2BAA2B,YAAa,CA7DrD,MA6DqD,mCAC3C,WAAqC,IAAI,IACzC,cAAuC,KACvC,UAAoB,GACpB,cAAwB,EACxB,cACA,UAAqB,MACrB,SAAoB,MAG5B,OAAgB,OAAS,CACvB,OAAQ,OAAC,GAAc,EAAf,UACR,WAAY,OAAC,GAAc,EAAI,EAAnB,cACZ,YAAa,OAAC,GAAc,GAAK,EAAI,GAAxB,eACb,cAAe,OAAC,GAAe,EAAI,GAAM,EAAI,EAAI,EAAI,IAAM,EAAI,EAAI,GAAK,EAAzD,iBACf,YAAa,OAAC,GAAc,EAAI,EAAI,EAAvB,eACb,aAAc,OAAC,GAAc,EAAE,EAAI,EAAI,EAAI,EAA7B,gBACd,eAAgB,OAAC,GACf,EAAI,GAAM,EAAI,EAAI,EAAI,GAAK,EAAI,IAAM,EAAI,EAAI,IAAM,EAAI,EAAI,GAAK,EADlD,kBAEhB,eAAgB,OAAC,GAAc,CAC7B,MAAM,EAAI,GACV,OAAO,KAAK,IAAI,EAAG,IAAM,CAAC,EAAI,KAAK,KAAK,EAAI,EAAI,IAAM,EAAI,KAAK,IAAM,CAAC,EAAI,CAC5E,EAHgB,iBAIlB,EAGA,OAAgB,cAAgB,CAC9B,KAAM,CAAC,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,QAAG,EACvD,MAAO,CAAC,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,QAAG,EAC9C,MAAO,CAAC,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,QAAG,EACxD,KAAM,CAAC,IAAK,KAAM,IAAK,GAAG,EAC1B,KAAM,CAAC,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,QAAG,EAC7C,WAAY,CAAC,MAAO,MAAO,MAAO,KAAK,EACvC,oBAAqB,CAAC,MAAO,MAAO,MAAO,MAAO,MAAO,KAAK,EAC9D,KAAM,CAAC,SAAK,SAAK,SAAK,SAAK,SAAK,QAAG,EACnC,UAAW,CAAC,SAAK,SAAK,QAAG,EACzB,aAAc,CAAC,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,QAAG,EAC/D,eAAgB,CAAC,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SAAK,SA
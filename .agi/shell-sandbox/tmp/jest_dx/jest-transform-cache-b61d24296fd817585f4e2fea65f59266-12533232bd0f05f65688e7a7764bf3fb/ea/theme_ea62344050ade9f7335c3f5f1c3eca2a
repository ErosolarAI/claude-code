  const glow = glowColor || baseColor;
  return {
    text: _chalk.default.hex(baseColor).bold,
    bright: _chalk.default.hex(glow).bold,
    dim: _chalk.default.hex(baseColor),
    bg: _chalk.default.bgHex(baseColor).hex('#FFFFFF')
  };
};


var _theme = require("./theme.js");
/**
 * Text Highlighter - Erosolar-CLI style
 * Applies sophisticated coloring to different text types
 */

/**
 * Highlight file paths (Erosolar-CLI style)
 * Example: src/ui/display.ts
 */
function highlightFilePath(path) {
  const parts = path.split('/');
  const file = parts.pop() || '';
  const dir = parts.join('/');
  if (dir) {
    return `${_theme.theme.ui.muted(`${dir}/`)}${_theme.theme.info(file)}`;
  }
  return _theme.theme.info(file);
}

/**
 * Highlight numbers in text
 * Example: "Read 150 lines" -> "Read [150] lines"
 */
function highlightNumbers(text) {
  return text.replace(/\b(\d+)\b/g, match => _theme.theme.ui.number(match));
}

/**
 * Highlight code blocks (inline)
 * Example: `const foo = 'bar'`
 */
function highlightInlineCode(text) {
  return text.replace(/`([^`]+)`/g, (_, code) => _theme.theme.ui.code(code));
}

/**
 * Highlight quoted strings
 * Example: "hello world"
 */
function highlightQuotedStrings(text) {
  return text.replace(/"([^"]*)"/g, (_, str) => _theme.theme.ui.string(`"${str}"`));
}

/**
 * Highlight keywords (Erosolar-CLI style)
 * Example: function, class, const, let, etc.
 */
function highlightKeywords(text) {
  const keywords = ['function', 'class', 'const', 'let', 'var', 'import', 'export', 'from', 'default', 'async', 'await', 'return', 'if', 'else', 'for', 'while', 'do', 'switch', 'case', 'try', 'catch', 'finally', 'throw', 'new', 'this', 'super', 'extends', 'implements'];
  let result = text;
  for (const keyword of keywords) {
    const regex = new RegExp(`\\b(${keyword})\\b`, 'g');
    result = result.replace(regex, match => _theme.theme.ui.keyword(match));
  }
  return result;
}

/**
 * Highlight URLs
 */
function highlightUrls(text) {
  const urlRegex = /(https?:\/\/[^\s]+)/g;
  return text.replace(urlRegex, url => _theme.theme.link.url(url));
}

/**
 * Highlight error messages (Erosolar-CLI style)
 * Makes errors stand out with proper coloring and clickable file references
 */
function highlightError(text) {
  // Highlight error type (TypeScript, JavaScript, Node errors)
  text = text.replace(/^(Error|TypeError|SyntaxError|ReferenceError|RangeError|URIError|EvalError):/im, match => _theme.theme.error.bold(match));

  // Highlight TypeScript errors (TS2xxx)
  text = text.replace(/error (TS\d+):/g, (_, code) => `error ${_theme.theme.error.bold(code)}:`);

  // Highlight file:line:col references (Erosolar-CLI style - make them look clickable)
  // Pattern: path/to/file.ts(10,5) or path/to/file.ts:10:5
  text = text.replace(/([a-zA-Z0-9_\-./]+\.[a-zA-Z]+)\((\d+),(\d+)\)/g, (_, file, line, col) => `${_theme.theme.info(file)}:${_theme.theme.warning(line)}:${_theme.theme.warning(col)}`);
  text = text.replace(/([a-zA-Z0-9_\-./]+\.[a-zA-Z]+):(\d+):(\d+)/g, (_, file, line, col) => `${_theme.theme.info(file)}:${_theme.theme.warning(line)}:${_theme.theme.warning(col)}`);

  // Highlight stack trace lines (at function in file:line:col)
  text = text.replace(/^\s+at\s+([^\s]+)\s+\(([^:]+):(\d+):(\d+)\)/gm, (_, fn, file, line, col) => `  at ${_theme.theme.ui.muted(fn)} (${_theme.theme.info(file)}:${_theme.theme.warning(line)}:${_theme.theme.warning(col)})`);

  // Highlight anonymous stack trace (at file:line:col without function)
  text = text.replace(/^\s+at\s+([^(]+):(\d+):(\d+)$/gm, (_, file, line, col) => `  at ${_theme.theme.info(file.trim())}:${_theme.theme.warning(line)}:${_theme.theme.warning(col)}`);

  // Highlight error keywords in the message
  const errorKeywords = ['failed', 'cannot', 'unable', 'invalid', 'missing', 'undefined', 'null', 'expected'];
  for (const keyword of errorKeywords) {
    const regex = new RegExp(`\\b(${keyword})\\b`, 'gi');
    text = text.replace(regex, match => _theme.theme.error(match));
  }
  return text;
}

/**
 * Format file:line reference for terminal output (Erosolar-CLI style)
 * Returns a formatted string that looks like a clickable reference
 */
function formatFileLineRef(filePath, line, col) {
  if (line !== undefined && col !== undefined) {
    return `${_theme.theme.info(filePath)}:${_theme.theme.warning(String(line))}:${_theme.theme.warning(String(col))}`;
  }
  if (line !== undefined) {
    return `${_theme.theme.info(filePath)}:${_theme.theme.warning(String(line))}`;
  }
  return _theme.theme.info(filePath);
}

/**
 * Highlight success messages
 */
function highlightSuccess(text) {
  const successWords = ['success', 'completed', 'passed', 'done', 'ok'];
  let result = text;
  for (const word of successWords) {
    const regex = new RegExp(`\\b(${word})\\b`, 'gi');
    result = result.replace(regex, match => _theme.theme.success.bold(match));
  }
  return result;
}

/**
 * Highlight warning messages
 */
function highlightWarning(text) {
  const warningWords = ['warning', 'deprecated', 'caution', 'attention'];
  let result = text;
  for (const word of warningWords) {
    const regex = new RegExp(`\\b(${word})\\b`, 'gi');
    result = result.replace(regex, match => _theme.theme.warning.bold(match));
  }
  return result;
}

/**
 * Highlight tool names (Erosolar-CLI style)
 */
function highlightToolName(name) {
  return _theme.theme.info.bold(name);
}

/**
 * Highlight command output (Erosolar-CLI style)
 * Applies context-aware highlighting
 */
function highlightCommandOutput(output, command) {
  let result = output;

  // Git commands
  if (command?.includes('git')) {
    result = highlightGitOutput(result);
  }

  // npm commands
  else if (command?.includes('npm')) {
    result = highlightNpmOutput(result);
  }

  // General highlighting
  else {
    result = highlightNumbers(result);
    result = highlightUrls(result);
    result = highlightFilePaths(result);
  }
  return result;
}

/**
 * Highlight file paths in text
 */
function highlightFilePaths(text) {
  // Match common file path patterns
  const pathRegex = /([a-zA-Z0-9_\-./]+\.(ts|js|tsx|jsx|json|md|txt|yml|yaml|css|scss|html))/g;
  return text.replace(pathRegex, match => highlightFilePath(match));
}

/**
 * Highlight git output
 */
function highlightGitOutput(text) {
  let result = text;

  // Branch names
  result = result.replace(/On branch (\w+)/g, (_, branch) => `On branch ${_theme.theme.info.bold(branch)}`);

  // File statuses
  result = result.replace(/^\s*modified:/gm, match => _theme.theme.warning(match));
  result = result.replace(/^\s*deleted:/gm, match => _theme.theme.error(match));
  result = result.replace(/^\s*new file:/gm, match => _theme.theme.success(match));

  // Ahead/behind
  result = result.replace(/ahead (\d+)/g, (_, num) => `ahead ${_theme.theme.success.bold(num)}`);
  result = result.replace(/behind (\d+)/g, (_, num) => `behind ${_theme.theme.warning.bold(num)}`);
  return result;
}

/**
 * Highlight npm output
 */
function highlightNpmOutput(text) {
  let result = text;

  // Package names
  result = result.replace(/(@[\w-]+\/[\w-]+|[\w-]+@[\d.]+)/g, match => _theme.theme.info(match));

  // Warnings
  result = result.replace(/WARN/g, _theme.theme.warning.bold('WARN'));

  // Errors
  result = result.replace(/ERR!/g, _theme.theme.error.bold('ERR!'));
  return result;
}

/**
 * Apply all highlighting (auto-detect)
 */
function highlightText(text, context) {
  let result = text;
  switch (context) {
    case 'error':
      result = highlightError(result);
      break;
    case 'code':
      result = highlightKeywords(result);
      result = highlightQuotedStrings(result);
      result = highlightNumbers(result);
      break;
    case 'command':
      result = highlightCommandOutput(result);
      break;
    case 'log':
      result = highlightSuccess(result);
      result = highlightWarning(result);
      result = highlightNumbers(result);
      break;
    default:
      // Auto-detect and apply all
      result = highlightNumbers(result);
      result = highlightUrls(result);
      result = highlightInlineCode(result);
      result = highlightSuccess(result);
      result = highlightWarning(result);
  }
  return result;
}

/**
 * Highlight important text (bold + colored)
 */
function highlightImportant(text) {
  return _theme.theme.ui.highlight(text);
}

/**
 * Emphasize text (less prominent than important)
 */
function emphasizeText(text) {
  return _theme.theme.ui.emphasis(text);
}

/**
 * Highlight thinking blocks (Erosolar-CLI style)
 * Wraps <thinking>...</thinking> content in a styled box
 */
function highlightThinkingBlock(text) {
  // Match <thinking>...</thinking> blocks
  const thinkingRegex = /<thinking>([\s\S]*?)<\/thinking>/gi;
  return text.replace(thinkingRegex, (_, content) => {
    const trimmedContent = content.trim();
    const lines = trimmedContent.split('\n');

    // Build the styled thinking box
    const header = _theme.theme.ui.muted('â”Œâ”€ ') + _theme.theme.secondary('ðŸ’­ Thinking') + _theme.theme.ui.muted(' â”€');
    const footer = _theme.theme.ui.muted(`â””${'â”€'.repeat(20)}`);
    const styledLines = lines.map(line => {
      const trimmedLine = line.trim();
      if (!trimmedLine) return _theme.theme.ui.muted('â”‚');
      return _theme.theme.ui.muted('â”‚ ') + _theme.theme.ui.muted(trimmedLine);
    });
    return [header, ...styledLines, footer].join('\n');
  });
}

/**
 * Format streaming thinking content (for incremental display)
 * Shows thinking in a muted italic style without the box
 */
function formatThinkingContent(content) {
  return _theme.theme.ui.muted(content);
}

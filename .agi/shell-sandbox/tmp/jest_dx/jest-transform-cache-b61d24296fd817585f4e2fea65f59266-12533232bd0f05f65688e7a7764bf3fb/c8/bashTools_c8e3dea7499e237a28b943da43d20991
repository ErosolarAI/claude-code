function findGuiLauncher(_command) {
  return null;
}
const errorFixerCache = new Map();
function getErrorFixer(workingDir) {
  let fixer = errorFixerCache.get(workingDir);
  if (!fixer) {
    fixer = (0, _aiErrorFixer.createErrorFixer)({
      workingDir
    });
    errorFixerCache.set(workingDir, fixer);
  }
  return fixer;
}

/**
 * Smart timeout detection based on command type
 */
function getSmartTimeout(command) {
  const cmd = command.toLowerCase().trim();

  // Long-running ML/AI training jobs (10 minutes)
  if (/\b(python|python3)\b/.test(cmd) && /\b(train|fine[-\s]?tune|finetune)\b/.test(cmd)) {
    return 600000;
  }
  if (/\b(torchrun|accelerate\s+launch)\b/.test(cmd)) return 600000;

  // Python/pytest test runners (5 minutes)
  if (/\b(pytest|python\s+-m\s+pytest)\b/.test(cmd)) return 300000;

  // NPM/Yarn/PNPM operations (5 minutes)
  if (/\b(npm|yarn|pnpm)\s+(install|ci|i|add|update|upgrade)\b/.test(cmd)) return 300000;
  if (/\b(npm|yarn|pnpm)\s+(run\s+)?(build|test|lint|start|dev)\b/.test(cmd)) return 300000;
  if (/\bnpx\s+(create-|degit|giget)\b/.test(cmd)) return 300000;

  // Prisma (3 minutes)
  if (/\b(prisma|npx prisma)\s+(generate|migrate|db push|db pull)\b/.test(cmd)) return 180000;

  // Docker (10 minutes)
  if (/\bdocker\s+(build|pull|push|compose)\b/.test(cmd)) return 600000;

  // Git clone / pip install (3 minutes)
  if (/\bgit\s+clone\b/.test(cmd)) return 180000;
  if (/\bpip\s+install\b/.test(cmd)) return 180000;
  return 30000; // Default
}

// ============================================================================
// Sandbox Environment
// ============================================================================

const sandboxCache = new Map();
async function ensureSandboxPaths(workingDir) {
  let pending = sandboxCache.get(workingDir);
  if (!pending) {
    pending = createSandboxPaths(workingDir);
    sandboxCache.set(workingDir, pending);
  }
  return pending;
}
async function createSandboxPaths(workingDir) {
  const root = (0, _nodePath.join)(workingDir, '.agi', 'shell-sandbox');
  const home = (0, _nodePath.join)(root, 'home');
  const cache = (0, _nodePath.join)(root, 'cache');
  const config = (0, _nodePath.join)(root, 'config');
  const data = (0, _nodePath.join)(root, 'data');
  const tmp = (0, _nodePath.join)(root, 'tmp');
  await Promise.all([home, cache, config, data, tmp].map(dir => (0, _promises.mkdir)(dir, {
    recursive: true
  })));
  return {
    root,
    home,
    cache,
    config,
    data,
    tmp
  };
}
async function buildSandboxEnv(workingDir, options) {
  const envPreference = process.env['AGI_PRESERVE_HOME'];
  const preserveHome = envPreference === '1' ? true : envPreference === '0' ? false : Boolean(options?.preserveHome);
  const paths = await ensureSandboxPaths(workingDir);
  const env = {
    ...process.env,
    AGI_SANDBOX_ROOT: paths.root,
    AGI_SANDBOX_HOME: paths.home,
    AGI_SANDBOX_TMP: paths.tmp
  };
  if (!preserveHome) env['HOME'] = paths.home;
  env['XDG_CACHE_HOME'] = paths.cache;
  env['XDG_CONFIG_HOME'] = paths.config;
  env['XDG_DATA_HOME'] = paths.data;
  env['TMPDIR'] = paths.tmp;
  env['TMP'] = paths.tmp;
  env['TEMP'] = paths.tmp;
  return env;
}


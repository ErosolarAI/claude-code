var _theme = require("./theme.js");
var _layout = require("./layout.js");
var _outputMode = require("./outputMode.js");
const toneColors = {
  neutral: _theme.theme.ui.text,
  info: _theme.theme.info,
  success: _theme.theme.success,
  warning: _theme.theme.warning,
  danger: _theme.theme.error,
  accent: _theme.theme.secondary
};
function renderStatusBar(segments, options = {}) {
  if (!segments.length) {
    return '';
  }
  const width = clampWidth(options.width ?? (0, _layout.getContentWidth)());
  const divider = _theme.theme.ui.muted(` ${_theme.icons.bullet} `);
  const chunks = segments.filter(segment => Boolean(segment.label?.trim() && segment.value?.trim())).map(segment => formatStatusChunk(segment));
  if (!chunks.length) {
    return '';
  }
  const lines = [];
  let current = '';
  const pushChunk = chunk => {
    if (!current) {
      current = chunk;
      return;
    }
    const candidate = `${current}${divider}${chunk}`;
    if ((0, _layout.measure)(candidate) > width) {
      lines.push(padLine(current, width));
      current = chunk;
    } else {
      current = candidate;
    }
  };
  chunks.forEach(pushChunk);
  if (current) {
    lines.push(padLine(current, width));
  }
  return lines.join('\n');
}
function renderCallout(message, options = {}) {
  const width = options.width ?? (0, _layout.getContentWidth)();
  const tone = options.tone ?? 'info';
  const icon = options.icon ?? _theme.icons.info;
  const title = options.title ?? capitalize(tone);
  const accent = toneColors[tone] ?? toneColors.info;
  const contentWidth = Math.max(24, (0, _layout.normalizePanelWidth)(width) - 4);

  // Split by newlines to preserve intentional line breaks
  const rawLines = message.split('\n');
  const paragraphs = [];
  for (const line of rawLines) {
    const trimmed = line.trim();
    if (!trimmed) {
      // Empty line - add a blank line to preserve spacing
      paragraphs.push('');
    } else {
      // Wrap this line
      const wrapped = (0, _layout.wrapParagraph)(trimmed, contentWidth);
      paragraphs.push(...wrapped);
    }
  }
  return (0, _layout.renderPanel)(paragraphs, {
    icon,
    title,
    accentColor: accent,
    borderColor: accent,
    width
  });
}
function renderSectionHeading(title, options = {}) {
  const width = clampWidth(options.width ?? (0, _layout.getContentWidth)());
  const accent = toneColors[options.tone ?? 'accent'] ?? toneColors.accent;
  const icon = options.icon ? `${options.icon} ` : '';
  const label = `${icon}${title}`.toUpperCase();

  // Plain output mode for clipboard-friendly text
  if ((0, _outputMode.isPlainOutputMode)()) {
    const subtitle = options.subtitle?.trim() ? `: ${_theme.theme.ui.muted(options.subtitle.trim())}` : '';
    return accent(`${label}${subtitle}`);
  }
  const underline = accent('â”'.repeat(width));
  const subtitle = options.subtitle?.trim() ? `${_theme.theme.ui.muted(options.subtitle.trim())}` : '';
  const lines = [underline, accent(padLine(label, width))];
  if (subtitle) {
    lines.push(padLine(subtitle, width));
  }
  return lines.join('\n');
}
function formatStatusChunk(segment) {
  const tone = toneColors[segment.tone ?? 'neutral'] ?? toneColors.neutral;
  const icon = segment.icon ? `${tone(segment.icon)} ` : '';
  const label = _theme.theme.ui.muted(segment.label.trim().toUpperCase());
  const value = tone(segment.value.trim());
  return `${icon}${label} ${value}`;
}
function padLine(text, width) {
  const visible = (0, _layout.measure)(text);
  if (visible >= width) {
    return text;
  }
  return `${text}${' '.repeat(width - visible)}`;
}
function clampWidth(value) {
  const normalized = Math.max(32, Math.floor(value));
  return Math.min(120, normalized);
}
function capitalize(value) {
  if (!value) {
    return '';
  }
  return value.slice(0, 1).toUpperCase() + value.slice(1);
}

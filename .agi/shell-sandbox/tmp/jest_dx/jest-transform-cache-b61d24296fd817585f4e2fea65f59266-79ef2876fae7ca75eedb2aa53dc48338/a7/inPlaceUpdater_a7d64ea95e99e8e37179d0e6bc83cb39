    /* istanbul ignore next */
    cov_2oml3m1efl().f[4]++;
    cov_2oml3m1efl().s[12]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[6][0]++;
      cov_2oml3m1efl().s[13]++;
      this.stream.write('\x1b[K');
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[6][1]++;
    }
  }

  /**
   * Clear entire line
   */
  clearLine() {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[5]++;
    cov_2oml3m1efl().s[14]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[7][0]++;
      cov_2oml3m1efl().s[15]++;
      this.stream.write('\r\x1b[K');
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[7][1]++;
    }
  }

  /**
   * Save cursor position
   */
  saveCursor() {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[6]++;
    cov_2oml3m1efl().s[16]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[8][0]++;
      cov_2oml3m1efl().s[17]++;
      this.stream.write('\x1b[s');
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[8][1]++;
    }
  }

  /**
   * Restore cursor position
   */
  restoreCursor() {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[7]++;
    cov_2oml3m1efl().s[18]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[9][0]++;
      cov_2oml3m1efl().s[19]++;
      this.stream.write('\x1b[u');
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[9][1]++;
    }
  }

  /**
   * Hide cursor
   */
  hideCursor() {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[8]++;
    cov_2oml3m1efl().s[20]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[10][0]++;
      cov_2oml3m1efl().s[21]++;
      this.stream.write('\x1b[?25l');
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[10][1]++;
    }
  }

  /**
   * Show cursor
   */
  showCursor() {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[9]++;
    cov_2oml3m1efl().s[22]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[11][0]++;
      cov_2oml3m1efl().s[23]++;
      this.stream.write('\x1b[?25h');
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[11][1]++;
    }
  }

  // --------------------------------------------------------------------------
  // IN-PLACE UPDATES
  // --------------------------------------------------------------------------

  /**
   * Create an updateable region
   */
  createRegion(id, initialContent) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[10]++;
    const content =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[24]++, Array.isArray(initialContent) ?
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[12][0]++, initialContent) :
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[12][1]++, [initialContent]));

    // Write initial content
    /* istanbul ignore next */
    cov_2oml3m1efl().s[25]++;
    for (const line of content) {
      /* istanbul ignore next */
      cov_2oml3m1efl().s[26]++;
      this.stream.write(`${line}\n`);
    }
    /* istanbul ignore next */
    cov_2oml3m1efl().s[27]++;
    this.regions.set(id, {
      id,
      startLine: 0,
      // We don't track absolute position, only relative
      lineCount: content.length,
      content,
      lastUpdate: Date.now()
    });
  }

  /**
   * Update a region in place
   */
  updateRegion(id, newContent) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[11]++;
    const region =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[28]++, this.regions.get(id));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[29]++;
    if (!region) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[13][0]++;
      cov_2oml3m1efl().s[30]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[13][1]++;
    }
    const content =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[31]++, Array.isArray(newContent) ?
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[14][0]++, newContent) :
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[14][1]++, [newContent]));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[32]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[15][0]++;
      cov_2oml3m1efl().s[33]++;
      // Move cursor up to the start of the region
      this.cursorUp(region.lineCount);

      // Write new content, clearing each line
      /* istanbul ignore next */
      cov_2oml3m1efl().s[34]++;
      for (let i =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[35]++, 0); i < Math.max(content.length, region.lineCount); i++) {
        /* istanbul ignore next */
        cov_2oml3m1efl().s[36]++;
        this.clearLine();
        /* istanbul ignore next */
        cov_2oml3m1efl().s[37]++;
        if (i < content.length) {
          /* istanbul ignore next */
          cov_2oml3m1efl().b[16][0]++;
          cov_2oml3m1efl().s[38]++;
          this.stream.write(`${content[i]}\n`);
        } else {
          /* istanbul ignore next */
          cov_2oml3m1efl().b[16][1]++;
          cov_2oml3m1efl().s[39]++;
          this.stream.write('\n'); // Clear extra lines
        }
      }

      // If new content is shorter, we need to move cursor down
      /* istanbul ignore next */
      cov_2oml3m1efl().s[40]++;
      if (content.length < region.lineCount) {
        /* istanbul ignore next */
        cov_2oml3m1efl().b[17][0]++;
      } // We're already at the right position
      else
      /* istanbul ignore next */
      {
        cov_2oml3m1efl().b[17][1]++;
      }
    } else {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[15][1]++;
      cov_2oml3m1efl().s[41]++;
      // Non-TTY fallback: just print new content
      for (const line of content) {
        /* istanbul ignore next */
        cov_2oml3m1efl().s[42]++;
        this.stream.write(`${line}\n`);
      }
    }

    // Update region metadata
    /* istanbul ignore next */
    cov_2oml3m1efl().s[43]++;
    region.content = content;
    /* istanbul ignore next */
    cov_2oml3m1efl().s[44]++;
    region.lineCount = content.length;
    /* istanbul ignore next */
    cov_2oml3m1efl().s[45]++;
    region.lastUpdate = Date.now();
  }

  /**
   * Delete a region
   */
  deleteRegion(id) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[12]++;
    const region =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[46]++, this.regions.get(id));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[47]++;
    if (!region) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[18][0]++;
      cov_2oml3m1efl().s[48]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[18][1]++;
    }
    cov_2oml3m1efl().s[49]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[19][0]++;
      cov_2oml3m1efl().s[50]++;
      // Move up and clear all lines
      this.cursorUp(region.lineCount);
      /* istanbul ignore next */
      cov_2oml3m1efl().s[51]++;
      for (let i =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[52]++, 0); i < region.lineCount; i++) {
        /* istanbul ignore next */
        cov_2oml3m1efl().s[53]++;
        this.clearLine();
        /* istanbul ignore next */
        cov_2oml3m1efl().s[54]++;
        if (i < region.lineCount - 1) {
          /* istanbul ignore next */
          cov_2oml3m1efl().b[20][0]++;
          cov_2oml3m1efl().s[55]++;
          this.cursorDown(1);
        } else
        /* istanbul ignore next */
        {
          cov_2oml3m1efl().b[20][1]++;
        }
      }
      // Move back up to where the region started
      /* istanbul ignore next */
      cov_2oml3m1efl().s[56]++;
      this.cursorUp(region.lineCount - 1);
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[19][1]++;
    }
    cov_2oml3m1efl().s[57]++;
    this.regions.delete(id);
  }

  /**
   * Update a single line in place (most common use case)
   */
  updateLine(content) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[13]++;
    cov_2oml3m1efl().s[58]++;
    if (this.isTTY) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[21][0]++;
      cov_2oml3m1efl().s[59]++;
      this.cursorToLineStart();
      /* istanbul ignore next */
      cov_2oml3m1efl().s[60]++;
      this.clearToEndOfLine();
      /* istanbul ignore next */
      cov_2oml3m1efl().s[61]++;
      this.stream.write(content);
    } else {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[21][1]++;
      cov_2oml3m1efl().s[62]++;
      this.stream.write(`${content}\n`);
    }
  }

  /**
   * Finalize a line update (add newline)
   */
  finalizeLine(content) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[14]++;
    cov_2oml3m1efl().s[63]++;
    if (content) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[22][0]++;
      cov_2oml3m1efl().s[64]++;
      this.updateLine(content);
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[22][1]++;
    }
    cov_2oml3m1efl().s[65]++;
    this.stream.write('\n');
  }

  // --------------------------------------------------------------------------
  // PROGRESS BAR
  // --------------------------------------------------------------------------

  /**
   * Create a progress bar string
   */
  formatProgressBar(current, total, options =
  /* istanbul ignore next */
  (cov_2oml3m1efl().b[23][0]++, {})) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[15]++;
    const {
      width =
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[24][0]++, 20),
      showPercentage =
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[25][0]++, true),
      showCount =
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[26][0]++, false),
      label,
      style =
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[27][0]++, 'bar')
    } =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[66]++, options);
    const percentage =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[67]++, Math.min(100, Math.max(0, Math.round(current / total * 100))));
    const filled =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[68]++, Math.round(current / total * width));
    const empty =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[69]++, width - filled);
    let bar;
    /* istanbul ignore next */
    cov_2oml3m1efl().s[70]++;
    switch (style) {
      case 'braille':
        /* istanbul ignore next */
        cov_2oml3m1efl().b[28][0]++;
        cov_2oml3m1efl().s[71]++;
        bar = this.formatBrailleBar(current, total, width);
        /* istanbul ignore next */
        cov_2oml3m1efl().s[72]++;
        break;
      case 'dots':
        /* istanbul ignore next */
        cov_2oml3m1efl().b[28][1]++;
        cov_2oml3m1efl().s[73]++;
        bar = ''.repeat(filled) + ''.repeat(empty);
        /* istanbul ignore next */
        cov_2oml3m1efl().s[74]++;
        break;
      case 'bar':
        /* istanbul ignore next */
        cov_2oml3m1efl().b[28][2]++;
      default:
        /* istanbul ignore next */
        cov_2oml3m1efl().b[28][3]++;
        cov_2oml3m1efl().s[75]++;
        bar = theme.progress.bar(progressChars.filled.repeat(filled)) + theme.progress.empty(progressChars.empty.repeat(empty));
    }
    const parts =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[76]++, []);
    /* istanbul ignore next */
    cov_2oml3m1efl().s[77]++;
    if (label) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[29][0]++;
      cov_2oml3m1efl().s[78]++;
      parts.push(theme.ui.muted(label));
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[29][1]++;
    }
    cov_2oml3m1efl().s[79]++;
    parts.push(`[${bar}]`);
    /* istanbul ignore next */
    cov_2oml3m1efl().s[80]++;
    if (showPercentage) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[30][0]++;
      cov_2oml3m1efl().s[81]++;
      parts.push(theme.progress.percentage(`${percentage}%`));
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[30][1]++;
    }
    cov_2oml3m1efl().s[82]++;
    if (showCount) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[31][0]++;
      cov_2oml3m1efl().s[83]++;
      parts.push(theme.ui.muted(`(${current}/${total})`));
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[31][1]++;
    }
    cov_2oml3m1efl().s[84]++;
    return parts.join(' ');
  }

  /**
   * Create a smooth braille-based progress bar
   */
  formatBrailleBar(current, total, width) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[16]++;
    const ratio =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[85]++, current / total);
    const fullBlocks =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[86]++, Math.floor(ratio * width));
    const remainder =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[87]++, ratio * width - fullBlocks);

    // Braille patterns for partial fill (0/8 to 8/8)
    const partials =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[88]++, [' ', '', '', '', '', '', '', '', '']);
    const partialIndex =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[89]++, Math.floor(remainder * 8));
    let bar =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[90]++, ''.repeat(fullBlocks));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[91]++;
    if (fullBlocks < width) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[32][0]++;
      cov_2oml3m1efl().s[92]++;
      bar += partials[partialIndex];
      /* istanbul ignore next */
      cov_2oml3m1efl().s[93]++;
      bar += ' '.repeat(width - fullBlocks - 1);
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[32][1]++;
    }
    cov_2oml3m1efl().s[94]++;
    return theme.progress.bar(bar);
  }

  /**
   * Update a progress bar in place
   */
  updateProgress(regionId, current, total, options =
  /* istanbul ignore next */
  (cov_2oml3m1efl().b[33][0]++, {})) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[17]++;
    const bar =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[95]++, this.formatProgressBar(current, total, options));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[96]++;
    this.updateLine(bar);
  }

  // --------------------------------------------------------------------------
  // SPINNER
  // --------------------------------------------------------------------------

  /**
   * Start a spinner
   */
  startSpinner(id, label, options =
  /* istanbul ignore next */
  (cov_2oml3m1efl().b[34][0]++, {})) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[18]++;
    cov_2oml3m1efl().s[97]++;
    if (this.spinnerInterval) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[35][0]++;
      cov_2oml3m1efl().s[98]++;
      this.stopSpinner();
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[35][1]++;
    }
    cov_2oml3m1efl().s[99]++;
    this.activeSpinnerId = id;
    const style =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[100]++,
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[36][0]++, options.style) ||
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[36][1]++, 'circle'));
    const frames =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[101]++, spinnerFrames[style]);

    // Initial render
    /* istanbul ignore next */
    cov_2oml3m1efl().s[102]++;
    this.renderSpinner(frames, label);

    // Start animation
    /* istanbul ignore next */
    cov_2oml3m1efl().s[103]++;
    this.spinnerInterval = setInterval(() => {
      /* istanbul ignore next */
      cov_2oml3m1efl().f[19]++;
      cov_2oml3m1efl().s[104]++;
      this.currentSpinnerFrame = (this.currentSpinnerFrame + 1) % frames.length;
      /* istanbul ignore next */
      cov_2oml3m1efl().s[105]++;
      this.renderSpinner(frames, label);
    }, 80);
  }

  /**
   * Update spinner label
   */
  updateSpinner(_label) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[20]++;
    cov_2oml3m1efl().s[106]++;
    if (!this.spinnerInterval) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[37][0]++;
      cov_2oml3m1efl().s[107]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[37][1]++;
    }
    // The next frame will pick up the new label
    // For immediate update, we'd need to store the current frames
  }

  /**
   * Stop spinner and show final message
   */
  stopSpinner(finalMessage, success =
  /* istanbul ignore next */
  (cov_2oml3m1efl().b[38][0]++, true)) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[21]++;
    cov_2oml3m1efl().s[108]++;
    if (this.spinnerInterval) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[39][0]++;
      cov_2oml3m1efl().s[109]++;
      clearInterval(this.spinnerInterval);
      /* istanbul ignore next */
      cov_2oml3m1efl().s[110]++;
      this.spinnerInterval = null;
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[39][1]++;
    }
    cov_2oml3m1efl().s[111]++;
    if (finalMessage) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[40][0]++;
      const icon =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[112]++, success ?
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[41][0]++, theme.success(icons.success)) :
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[41][1]++, theme.error(icons.error)));
      /* istanbul ignore next */
      cov_2oml3m1efl().s[113]++;
      this.finalizeLine(`${icon} ${finalMessage}`);
    } else {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[40][1]++;
      cov_2oml3m1efl().s[114]++;
      this.finalizeLine();
    }
    /* istanbul ignore next */
    cov_2oml3m1efl().s[115]++;
    this.activeSpinnerId = null;
    /* istanbul ignore next */
    cov_2oml3m1efl().s[116]++;
    this.currentSpinnerFrame = 0;
  }

  /**
   * Render current spinner frame
   */
  renderSpinner(frames, label) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[22]++;
    const frame =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[117]++, theme.info(frames[this.currentSpinnerFrame]));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[118]++;
    this.updateLine(`${frame} ${label}`);
  }

  // --------------------------------------------------------------------------
  // STREAMING UPDATES
  // --------------------------------------------------------------------------

  /**
   * Create a streaming update region that shows the last N lines
   */
  createStreamingRegion(id, options =
  /* istanbul ignore next */
  (cov_2oml3m1efl().b[42][0]++, {})) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[23]++;
    const {
      maxLines =
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[43][0]++, 5),
      truncate =
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[44][0]++, true),
      preserveLastLine =
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[45][0]++, true)
    } =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[119]++, options);
    const lines =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[120]++, []);

    // Create initial empty region
    const emptyLines =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[121]++, Array(maxLines).fill(''));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[122]++;
    this.createRegion(id, emptyLines);

    // Return updater function
    /* istanbul ignore next */
    cov_2oml3m1efl().s[123]++;
    return content => {
      /* istanbul ignore next */
      cov_2oml3m1efl().f[24]++;
      // Add new content (may contain newlines)
      const newLines =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[124]++, content.split('\n'));
      /* istanbul ignore next */
      cov_2oml3m1efl().s[125]++;
      for (const line of newLines) {
        /* istanbul ignore next */
        cov_2oml3m1efl().s[126]++;
        if (
        /* istanbul ignore next */
        (cov_2oml3m1efl().b[47][0]++, line) ||
        /* istanbul ignore next */
        (cov_2oml3m1efl().b[47][1]++, !preserveLastLine)) {
          /* istanbul ignore next */
          cov_2oml3m1efl().b[46][0]++;
          cov_2oml3m1efl().s[127]++;
          lines.push(line);
        } else
        /* istanbul ignore next */
        {
          cov_2oml3m1efl().b[46][1]++;
        }
      }

      // Keep only last maxLines
      /* istanbul ignore next */
      cov_2oml3m1efl().s[128]++;
      while (lines.length > maxLines) {
        /* istanbul ignore next */
        cov_2oml3m1efl().s[129]++;
        lines.shift();
      }

      // Pad to maxLines for consistent region size
      const displayLines =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[130]++, [...lines]);
      /* istanbul ignore next */
      cov_2oml3m1efl().s[131]++;
      while (displayLines.length < maxLines) {
        /* istanbul ignore next */
        cov_2oml3m1efl().s[132]++;
        displayLines.unshift('');
      }

      // Truncate lines if needed
      const termWidth =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[133]++, getTerminalColumns() - 2);
      const truncatedLines =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[134]++, truncate ?
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[48][0]++, displayLines.map(l => {
        /* istanbul ignore next */
        cov_2oml3m1efl().f[25]++;
        const visible =
        /* istanbul ignore next */
        (cov_2oml3m1efl().s[135]++, measure(l));
        /* istanbul ignore next */
        cov_2oml3m1efl().s[136]++;
        if (visible > termWidth) {
          /* istanbul ignore next */
          cov_2oml3m1efl().b[49][0]++;
          cov_2oml3m1efl().s[137]++;
          return `${stripAnsi(l).slice(0, termWidth - 3)}...`;
        } else
        /* istanbul ignore next */
        {
          cov_2oml3m1efl().b[49][1]++;
        }
        cov_2oml3m1efl().s[138]++;
        return l;
      })) :
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[48][1]++, displayLines));
      /* istanbul ignore next */
      cov_2oml3m1efl().s[139]++;
      this.updateRegion(id, truncatedLines);
    };
  }

  /**
   * Finalize streaming region with summary
   */
  finalizeStreamingRegion(id, summary) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[26]++;
    cov_2oml3m1efl().s[140]++;
    this.deleteRegion(id);
    /* istanbul ignore next */
    cov_2oml3m1efl().s[141]++;
    if (summary) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[50][0]++;
      cov_2oml3m1efl().s[142]++;
      this.stream.write(`${summary}\n`);
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[50][1]++;
    }
  }

  // --------------------------------------------------------------------------
  // COMPOUND DISPLAYS
  // --------------------------------------------------------------------------

  /**
   * Show tool execution with in-place progress
   */
  showToolProgress(toolName, message, progress) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[27]++;
    const parts =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[143]++, []);

    // Spinner or progress icon
    /* istanbul ignore next */
    cov_2oml3m1efl().s[144]++;
    if (progress) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[51][0]++;
      const pct =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[145]++, Math.round(progress.current / progress.total * 100));
      /* istanbul ignore next */
      cov_2oml3m1efl().s[146]++;
      parts.push(theme.info(`${pct}%`));
    } else {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[51][1]++;
      const frame =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[147]++, spinnerFrames.circle[this.currentSpinnerFrame]);
      /* istanbul ignore next */
      cov_2oml3m1efl().s[148]++;
      parts.push(theme.info(frame));
    }

    // Tool name
    /* istanbul ignore next */
    cov_2oml3m1efl().s[149]++;
    parts.push(theme.tool(toolName));

    // Message
    /* istanbul ignore next */
    cov_2oml3m1efl().s[150]++;
    parts.push(message);

    // Progress bar if applicable
    /* istanbul ignore next */
    cov_2oml3m1efl().s[151]++;
    if (progress) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[52][0]++;
      const bar =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[152]++, this.formatProgressBar(progress.current, progress.total, {
        width: 15,
        showPercentage: false
      }));
      /* istanbul ignore next */
      cov_2oml3m1efl().s[153]++;
      parts.push(bar);
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[52][1]++;
    }
    cov_2oml3m1efl().s[154]++;
    this.updateLine(parts.join(' '));
  }

  /**
   * Show file operation progress
   */
  showFileProgress(operation, filePath, progress) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[28]++;
    const opIcons =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[155]++, {
      read: icons.read,
      write: icons.write,
      edit: icons.edit
    });
    const shortPath =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[156]++, this.truncatePath(filePath, 40));
    const parts =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[157]++, [theme.info(
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[53][0]++, opIcons[operation]) ||
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[53][1]++, icons.file)), theme.file.path(shortPath)]);
    /* istanbul ignore next */
    cov_2oml3m1efl().s[158]++;
    if (progress) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[54][0]++;
      cov_2oml3m1efl().s[159]++;
      parts.push(this.formatProgressBar(progress.current, progress.total, {
        width: 12,
        showPercentage: true
      }));
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[54][1]++;
    }
    cov_2oml3m1efl().s[160]++;
    this.updateLine(parts.join(' '));
  }

  /**
   * Complete file operation with summary
   */
  completeFileOperation(operation, filePath, stats) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[29]++;
    const shortPath =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[161]++, this.truncatePath(filePath, 40));
    const parts =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[162]++, [theme.success(icons.success), theme.file.path(shortPath)]);
    /* istanbul ignore next */
    cov_2oml3m1efl().s[163]++;
    if (stats) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[55][0]++;
      const details =
      /* istanbul ignore next */
      (cov_2oml3m1efl().s[164]++, []);
      /* istanbul ignore next */
      cov_2oml3m1efl().s[165]++;
      if (stats.lines !== undefined) {
        /* istanbul ignore next */
        cov_2oml3m1efl().b[56][0]++;
        cov_2oml3m1efl().s[166]++;
        details.push(`${stats.lines} lines`);
      } else
      /* istanbul ignore next */
      {
        cov_2oml3m1efl().b[56][1]++;
      }
      cov_2oml3m1efl().s[167]++;
      if (
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[58][0]++, stats.additions !== undefined) &&
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[58][1]++, stats.additions > 0)) {
        /* istanbul ignore next */
        cov_2oml3m1efl().b[57][0]++;
        cov_2oml3m1efl().s[168]++;
        details.push(theme.file.additions(`+${stats.additions}`));
      } else
      /* istanbul ignore next */
      {
        cov_2oml3m1efl().b[57][1]++;
      }
      cov_2oml3m1efl().s[169]++;
      if (
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[60][0]++, stats.removals !== undefined) &&
      /* istanbul ignore next */
      (cov_2oml3m1efl().b[60][1]++, stats.removals > 0)) {
        /* istanbul ignore next */
        cov_2oml3m1efl().b[59][0]++;
        cov_2oml3m1efl().s[170]++;
        details.push(theme.file.removals(`-${stats.removals}`));
      } else
      /* istanbul ignore next */
      {
        cov_2oml3m1efl().b[59][1]++;
      }
      cov_2oml3m1efl().s[171]++;
      if (details.length > 0) {
        /* istanbul ignore next */
        cov_2oml3m1efl().b[61][0]++;
        cov_2oml3m1efl().s[172]++;
        parts.push(theme.ui.muted(`(${details.join(', ')})`));
      } else
      /* istanbul ignore next */
      {
        cov_2oml3m1efl().b[61][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[55][1]++;
    }
    cov_2oml3m1efl().s[173]++;
    this.finalizeLine(parts.join(' '));
  }

  // --------------------------------------------------------------------------
  // UTILITIES
  // --------------------------------------------------------------------------

  truncatePath(path, maxLen) {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[30]++;
    cov_2oml3m1efl().s[174]++;
    if (path.length <= maxLen) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[62][0]++;
      cov_2oml3m1efl().s[175]++;
      return path;
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[62][1]++;
    }
    const parts =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[176]++, path.split('/').filter(Boolean));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[177]++;
    if (parts.length <= 2) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[63][0]++;
      cov_2oml3m1efl().s[178]++;
      return `${path.slice(0, maxLen - 3)}...`;
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[63][1]++;
    }
    const filename =
    /* istanbul ignore next */
    (cov_2oml3m1efl().s[179]++,
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[64][0]++, parts[parts.length - 1]) ||
    /* istanbul ignore next */
    (cov_2oml3m1efl().b[64][1]++, ''));
    /* istanbul ignore next */
    cov_2oml3m1efl().s[180]++;
    if (filename.length <= maxLen - 4) {
      /* istanbul ignore next */
      cov_2oml3m1efl().b[65][0]++;
      cov_2oml3m1efl().s[181]++;
      return `.../${filename}`;
    } else
    /* istanbul ignore next */
    {
      cov_2oml3m1efl().b[65][1]++;
    }
    cov_2oml3m1efl().s[182]++;
    return `${filename.slice(0, maxLen - 3)}...`;
  }

  /**
   * Check if TTY is available
   */
  get hasTTY() {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[31]++;
    cov_2oml3m1efl().s[183]++;
    return this.isTTY;
  }

  /**
   * Cleanup resources
   */
  dispose() {
    /* istanbul ignore next */
    cov_2oml3m1efl().f[32]++;
    cov_2oml3m1efl().s[184]++;
    this.stopSpinner();
    /* istanbul ignore next */
    cov_2oml3m1efl().s[185]++;
    this.showCursor();
    /* istanbul ignore next */
    cov_2oml3m1efl().s[186]++;
    this.regions.clear();
  }
}

// ============================================================================
// SINGLETON INSTANCE
// ============================================================================

export const inPlaceUpdater =
/* istanbul ignore next */
(cov_2oml3m1efl().s[187]++, new InPlaceUpdater());

// ============================================================================
// CONVENIENCE FUNCTIONS
// ============================================================================

/**
 * Quick in-place line update
 */
export function updateInPlace(content) {
  /* istanbul ignore next */
  cov_2oml3m1efl().f[33]++;
  cov_2oml3m1efl().s[188]++;
  inPlaceUpdater.updateLine(content);
}

/**
 * Quick progress bar
 */
export function showProgress(current, total, label) {
  /* istanbul ignore next */
  cov_2oml3m1efl().f[34]++;
  const bar =
  /* istanbul ignore next */
  (cov_2oml3m1efl().s[189]++, inPlaceUpdater.formatProgressBar(current, total, {
    label,
    showPercentage: true,
    width: 25
  }));
  /* istanbul ignore next */
  cov_2oml3m1efl().s[190]++;
  inPlaceUpdater.updateLine(bar);
}

/**
 * Finalize progress with result
 */
export function finishProgress(message, success =
/* istanbul ignore next */
(cov_2oml3m1efl().b[66][0]++, true)) {
  /* istanbul ignore next */
  cov_2oml3m1efl().f[35]++;
  const icon =
  /* istanbul ignore next */
  (cov_2oml3m1efl().s[191]++, success ?
  /* istanbul ignore next */
  (cov_2oml3m1efl().b[67][0]++, theme.success(icons.success)) :
  /* istanbul ignore next */
  (cov_2oml3m1efl().b[67][1]++, theme.error(icons.error)));
  /* istanbul ignore next */
  cov_2oml3m1efl().s[192]++;
  inPlaceUpdater.finalizeLine(`${icon} ${message}`);
}

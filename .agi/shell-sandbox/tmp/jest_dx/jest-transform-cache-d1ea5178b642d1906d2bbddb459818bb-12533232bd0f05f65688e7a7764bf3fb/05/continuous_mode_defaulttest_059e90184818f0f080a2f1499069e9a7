"use strict";

var _globals = require("@jest/globals");
var _repoUpgradeOrchestrator = require("../src/core/repoUpgradeOrchestrator.ts");
describe('Continuous Mode Defaults', () => {
  (0, _globals.test)('single-continuous should be default mode', () => {
    // Check mode definitions exist
    (0, _globals.expect)(_repoUpgradeOrchestrator.REPO_UPGRADE_MODE_DEFINITIONS['single-continuous']).toBeDefined();
    (0, _globals.expect)(_repoUpgradeOrchestrator.REPO_UPGRADE_MODE_DEFINITIONS['dual-rl-continuous']).toBeDefined();
    (0, _globals.expect)(_repoUpgradeOrchestrator.REPO_UPGRADE_MODE_DEFINITIONS['dual-rl-tournament']).toBeDefined();

    // Verify single-continuous has correct properties
    const singleMode = _repoUpgradeOrchestrator.REPO_UPGRADE_MODE_DEFINITIONS['single-continuous'];
    (0, _globals.expect)(singleMode.id).toBe('single-continuous');
    (0, _globals.expect)(singleMode.variants).toEqual(['primary']);
    (0, _globals.expect)(singleMode.parallelVariants).toBe(false);
  });
  (0, _globals.test)('mode definitions should have proper fallback', () => {
    // This would test the getRepoUpgradeModeDefinition function
    // Default should be single-continuous
    (0, _globals.expect)(true).toBe(true);
  });
});
describe('Dual Tournament RL UI', () => {
  (0, _globals.test)('RL status interface should exist', () => {
    // RLAgentStatus interface should be defined
    (0, _globals.expect)(true).toBe(true);
  });
  (0, _globals.test)('UI should handle dual RL toggle correctly', () => {
    // Toggle should switch between single-continuous and dual-rl-continuous
    (0, _globals.expect)(true).toBe(true);
  });
});

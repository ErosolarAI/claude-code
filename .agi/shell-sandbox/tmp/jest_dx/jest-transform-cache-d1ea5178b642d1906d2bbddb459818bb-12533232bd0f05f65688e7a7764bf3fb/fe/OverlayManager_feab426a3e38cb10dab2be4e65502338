      (cov_1dr6je8km0().b[17][1]++, 0));
    }));

    // Compose lines from regions
    /* istanbul ignore next */
    cov_1dr6je8km0().s[54]++;
    for (const [, region] of sortedRegions) {
      /* istanbul ignore next */
      cov_1dr6je8km0().s[55]++;
      if (
      /* istanbul ignore next */
      (cov_1dr6je8km0().b[19][0]++, region) &&
      /* istanbul ignore next */
      (cov_1dr6je8km0().b[19][1]++, region.content)) {
        /* istanbul ignore next */
        cov_1dr6je8km0().b[18][0]++;
        const regionLines =
        /* istanbul ignore next */
        (cov_1dr6je8km0().s[56]++, region.content.split('\n').slice(0, region.height));
        /* istanbul ignore next */
        cov_1dr6je8km0().s[57]++;
        lines.push(...regionLines);

        // Respect max height
        /* istanbul ignore next */
        cov_1dr6je8km0().s[58]++;
        if (lines.length >= this.currentLayout.maxHeight) {
          /* istanbul ignore next */
          cov_1dr6je8km0().b[20][0]++;
          cov_1dr6je8km0().s[59]++;
          return lines.slice(0, this.currentLayout.maxHeight);
        } else
        /* istanbul ignore next */
        {
          cov_1dr6je8km0().b[20][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_1dr6je8km0().b[18][1]++;
      }
    }
    /* istanbul ignore next */
    cov_1dr6je8km0().s[60]++;
    return lines;
  }

  /**
   * Render the overlay to the terminal
   */
  render() {
    /* istanbul ignore next */
    cov_1dr6je8km0().f[13]++;
    cov_1dr6je8km0().s[61]++;
    if (
    /* istanbul ignore next */
    (cov_1dr6je8km0().b[22][0]++, !this.isVisible) ||
    /* istanbul ignore next */
    (cov_1dr6je8km0().b[22][1]++, !this.currentLayout)) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[21][0]++;
      cov_1dr6je8km0().s[62]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[21][1]++;
    }
    const lines =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[63]++, this.composeOverlay());
    /* istanbul ignore next */
    cov_1dr6je8km0().s[64]++;
    if (lines.length === 0) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[23][0]++;
      cov_1dr6je8km0().s[65]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[23][1]++;
    }
    const terminalWidth =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[66]++,
    /* istanbul ignore next */
    (cov_1dr6je8km0().b[24][0]++, this.writeStream.columns) ||
    /* istanbul ignore next */
    (cov_1dr6je8km0().b[24][1]++, 80));

    // Clear any previously rendered overlay
    /* istanbul ignore next */
    cov_1dr6je8km0().s[67]++;
    this.clearRenderedOverlay();

    // Allocate space for the overlay
    /* istanbul ignore next */
    cov_1dr6je8km0().s[68]++;
    this.allocatedRows = lines.length;
    /* istanbul ignore next */
    cov_1dr6je8km0().s[69]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[70]++, 0); i < this.allocatedRows; i++) {
      /* istanbul ignore next */
      cov_1dr6je8km0().s[71]++;
      this.writeStream.write('\n');
    }

    // Save cursor and move up to start of overlay area
    /* istanbul ignore next */
    cov_1dr6je8km0().s[72]++;
    if (this.options.preserveCursor) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[25][0]++;
      cov_1dr6je8km0().s[73]++;
      this.writeStream.write(this.SAVE_CURSOR);
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[25][1]++;
    }
    cov_1dr6je8km0().s[74]++;
    this.writeStream.write(this.MOVE_UP(this.allocatedRows));

    // Render each line
    /* istanbul ignore next */
    cov_1dr6je8km0().s[75]++;
    lines.forEach((line, index) => {
      /* istanbul ignore next */
      cov_1dr6je8km0().f[14]++;
      cov_1dr6je8km0().s[76]++;
      // Clear the line
      this.writeStream.write(this.CLEAR_LINE);
      /* istanbul ignore next */
      cov_1dr6je8km0().s[77]++;
      this.writeStream.write(this.MOVE_TO_COLUMN(1));

      // Truncate line if needed
      const truncatedLine =
      /* istanbul ignore next */
      (cov_1dr6je8km0().s[78]++, this.truncateLine(line, terminalWidth));
      /* istanbul ignore next */
      cov_1dr6je8km0().s[79]++;
      this.writeStream.write(truncatedLine);

      // Move to next line (except for last line)
      /* istanbul ignore next */
      cov_1dr6je8km0().s[80]++;
      if (index < lines.length - 1) {
        /* istanbul ignore next */
        cov_1dr6je8km0().b[26][0]++;
        cov_1dr6je8km0().s[81]++;
        this.writeStream.write('\n');
      } else
      /* istanbul ignore next */
      {
        cov_1dr6je8km0().b[26][1]++;
      }
    });

    // Restore cursor position
    /* istanbul ignore next */
    cov_1dr6je8km0().s[82]++;
    if (this.options.preserveCursor) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[27][0]++;
      cov_1dr6je8km0().s[83]++;
      this.writeStream.write(this.RESTORE_CURSOR);
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[27][1]++;
    }
    cov_1dr6je8km0().s[84]++;
    this.renderedRows = lines.length;
  }

  /**
   * Clear the rendered overlay from the terminal
   */
  clearRenderedOverlay() {
    /* istanbul ignore next */
    cov_1dr6je8km0().f[15]++;
    cov_1dr6je8km0().s[85]++;
    if (this.renderedRows === 0) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[28][0]++;
      cov_1dr6je8km0().s[86]++;
      return;
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[28][1]++;
    }

    // Save cursor position
    cov_1dr6je8km0().s[87]++;
    if (this.options.preserveCursor) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[29][0]++;
      cov_1dr6je8km0().s[88]++;
      this.writeStream.write(this.SAVE_CURSOR);
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[29][1]++;
    }

    // Move up to the start of the overlay
    cov_1dr6je8km0().s[89]++;
    this.writeStream.write(this.MOVE_UP(this.renderedRows));

    // Clear each overlay row
    /* istanbul ignore next */
    cov_1dr6je8km0().s[90]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[91]++, 0); i < this.renderedRows; i++) {
      /* istanbul ignore next */
      cov_1dr6je8km0().s[92]++;
      this.writeStream.write(this.CLEAR_LINE);
      /* istanbul ignore next */
      cov_1dr6je8km0().s[93]++;
      if (i < this.renderedRows - 1) {
        /* istanbul ignore next */
        cov_1dr6je8km0().b[30][0]++;
        cov_1dr6je8km0().s[94]++;
        this.writeStream.write('\n');
      } else
      /* istanbul ignore next */
      {
        cov_1dr6je8km0().b[30][1]++;
      }
    }

    // Move back up and restore cursor
    /* istanbul ignore next */
    cov_1dr6je8km0().s[95]++;
    if (this.renderedRows > 1) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[31][0]++;
      cov_1dr6je8km0().s[96]++;
      this.writeStream.write(this.MOVE_UP(this.renderedRows - 1));
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[31][1]++;
    }
    cov_1dr6je8km0().s[97]++;
    if (this.options.preserveCursor) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[32][0]++;
      cov_1dr6je8km0().s[98]++;
      this.writeStream.write(this.RESTORE_CURSOR);
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[32][1]++;
    }

    // Adjust cursor position if needed
    cov_1dr6je8km0().s[99]++;
    if (this.allocatedRows > 0) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[33][0]++;
      cov_1dr6je8km0().s[100]++;
      this.writeStream.write(this.MOVE_UP(this.allocatedRows));
      /* istanbul ignore next */
      cov_1dr6je8km0().s[101]++;
      this.allocatedRows = 0;
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[33][1]++;
    }
    cov_1dr6je8km0().s[102]++;
    this.renderedRows = 0;
  }

  /**
   * Get maximum overlay height based on terminal size
   */
  getMaxOverlayHeight() {
    /* istanbul ignore next */
    cov_1dr6je8km0().f[16]++;
    cov_1dr6je8km0().s[103]++;
    if (!this.options.adaptToTerminalSize) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[34][0]++;
      cov_1dr6je8km0().s[104]++;
      return 5; // Default max height
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[34][1]++;
    }
    const terminalHeight =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[105]++,
    /* istanbul ignore next */
    (cov_1dr6je8km0().b[35][0]++, this.writeStream.rows) ||
    /* istanbul ignore next */
    (cov_1dr6je8km0().b[35][1]++, 24));
    // Use at most 20% of terminal height for overlay
    /* istanbul ignore next */
    cov_1dr6je8km0().s[106]++;
    return Math.max(2, Math.min(5, Math.floor(terminalHeight * 0.2)));
  }

  /**
   * Truncate a line to fit terminal width
   */
  truncateLine(line, maxWidth) {
    /* istanbul ignore next */
    cov_1dr6je8km0().f[17]++;
    // Account for ANSI escape codes
    const visibleLength =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[107]++, stripAnsi(line).length);
    /* istanbul ignore next */
    cov_1dr6je8km0().s[108]++;
    if (visibleLength <= maxWidth) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[36][0]++;
      cov_1dr6je8km0().s[109]++;
      return line;
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[36][1]++;
    }

    // Smart truncation with ellipsis
    const ellipsis =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[110]++, '...');
    const targetLength =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[111]++, maxWidth - ellipsis.length);

    // Try to preserve ANSI codes while truncating
    let currentLength =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[112]++, 0);
    let result =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[113]++, '');
    let inAnsi =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[114]++, false);
    /* istanbul ignore next */
    cov_1dr6je8km0().s[115]++;
    for (let i =
    /* istanbul ignore next */
    (cov_1dr6je8km0().s[116]++, 0); i < line.length; i++) {
      const char =
      /* istanbul ignore next */
      (cov_1dr6je8km0().s[117]++, line[i]);
      /* istanbul ignore next */
      cov_1dr6je8km0().s[118]++;
      if (char === '\u001b') {
        /* istanbul ignore next */
        cov_1dr6je8km0().b[37][0]++;
        cov_1dr6je8km0().s[119]++;
        inAnsi = true;
      } else
      /* istanbul ignore next */
      {
        cov_1dr6je8km0().b[37][1]++;
      }
      cov_1dr6je8km0().s[120]++;
      if (inAnsi) {
        /* istanbul ignore next */
        cov_1dr6je8km0().b[38][0]++;
        cov_1dr6je8km0().s[121]++;
        result += char;
        /* istanbul ignore next */
        cov_1dr6je8km0().s[122]++;
        if (char === 'm') {
          /* istanbul ignore next */
          cov_1dr6je8km0().b[39][0]++;
          cov_1dr6je8km0().s[123]++;
          inAnsi = false;
        } else
        /* istanbul ignore next */
        {
          cov_1dr6je8km0().b[39][1]++;
        }
      } else {
        /* istanbul ignore next */
        cov_1dr6je8km0().b[38][1]++;
        cov_1dr6je8km0().s[124]++;
        if (currentLength < targetLength) {
          /* istanbul ignore next */
          cov_1dr6je8km0().b[40][0]++;
          cov_1dr6je8km0().s[125]++;
          result += char;
          /* istanbul ignore next */
          cov_1dr6je8km0().s[126]++;
          currentLength++;
        } else {
          /* istanbul ignore next */
          cov_1dr6je8km0().b[40][1]++;
          cov_1dr6je8km0().s[127]++;
          break;
        }
      }
    }
    /* istanbul ignore next */
    cov_1dr6je8km0().s[128]++;
    return result + ellipsis;
  }

  /**
   * Force a refresh of the overlay
   */
  refresh() {
    /* istanbul ignore next */
    cov_1dr6je8km0().f[18]++;
    cov_1dr6je8km0().s[129]++;
    if (this.isVisible) {
      /* istanbul ignore next */
      cov_1dr6je8km0().b[41][0]++;
      cov_1dr6je8km0().s[130]++;
      this.render();
    } else
    /* istanbul ignore next */
    {
      cov_1dr6je8km0().b[41][1]++;
    }
  }

  /**
   * Get current overlay state
   */
  getState() {
    /* istanbul ignore next */
    cov_1dr6je8km0().f[19]++;
    cov_1dr6je8km0().s[131]++;
    return {
      isVisible: this.isVisible,
      isEnabled: this.isEnabled,
      renderedRows: this.renderedRows,
      hasLayout: this.currentLayout !== null
    };
  }

  /**
   * Clean up resources
   */
  dispose() {
    /* istanbul ignore next */
    cov_1dr6je8km0().f[20]++;
    cov_1dr6je8km0().s[132]++;
    this.hide();
    /* istanbul ignore next */
    cov_1dr6je8km0().s[133]++;
    this.currentLayout = null;
  }
